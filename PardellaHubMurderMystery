NoclipButton.Name = "NoclipButton"
NoclipButton.Parent = MainTab
NoclipButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
NoclipButton.BackgroundTransparency = 1.000
NoclipButton.Position = UDim2.new(0.555249989, 0, 0.0827729702, 0)
NoclipButton.Size = UDim2.new(0, 32, 0, 32)
NoclipButton.Image = "rbxassetid://2790389767"
NoclipButton.ImageColor3 = Color3.fromRGB(91, 91, 91)
NoclipButton.ScaleType = Enum.ScaleType.Slice
NoclipButton.SliceCenter = Rect.new(8, 8, 248, 248)
NoclipButton.MouseButton1Click:connect(function()
	nam = game.Players.LocalPlayer.Name
	
	game:GetService('RunService').Stepped:connect(function()
		for a, b in pairs(game.Workspace:GetChildren()) do
			if b.Name == nam then
				for i, v in pairs(game.Workspace[nam]:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = false
						
					end
				end
			end
		end
	end)
	
	Workspace[nam].Head.Changed:connect(function()
		for a, b in pairs(game.Workspace:GetChildren()) do
			if b.Name == nam then
				for i, v in pairs(game.Workspace[nam]:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = false
						
					end
				end
			end
		end
	end)
end)

