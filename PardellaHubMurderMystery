FindMurdererButton.Name = "FindMurdererButton"
FindMurdererButton.Parent = MainTab
FindMurdererButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
FindMurdererButton.BackgroundTransparency = 1.000
FindMurdererButton.Position = UDim2.new(0.0391209498, 0, 0.376423776, 0)
FindMurdererButton.Size = UDim2.new(0, 32, 0, 32)
FindMurdererButton.Image = "rbxassetid://2790389767"
FindMurdererButton.ImageColor3 = Color3.fromRGB(91, 91, 91)
FindMurdererButton.ScaleType = Enum.ScaleType.Slice
FindMurdererButton.SliceCenter = Rect.new(8, 8, 248, 248)
FindMurdererButton.MouseButton1Click:connect(function()
	playas = game.Players:GetDescendants()
	local murderer = nil
	local sheriff = nil
	
	for i,v in ipairs(playas) do
		if v:IsA("Player") then
			find = v.Character:FindFirstChild("Knife")
			find2 = v.Character:FindFirstChild("Revolver")
			
			if find then
				murderer = v.Name
			end
			
			if find2 then
				sheriff = v.Name
			end
		end
		
		if v.Name == "Knife" then
			murderer = v.Parent.Parent.Name 
		end
		
		if v.Name == "Revolver" then
			sheriff = v.Parent.Parent.Name 
		end
		
	end
	
	if murderer then
		
		print("Murderer: " .. murderer)
		
		local tag = Instance.new("BillboardGui")
		tag.Parent = game.Players[murderer].Character.Head
		tag.Adornee = game.Players[murderer].Character.Head
		tag.StudsOffset = Vector3.new(0,3,0)
		tag.Size = UDim2.new(0,150,0,50)
		tag.AlwaysOnTop = true
		
		local title = Instance.new("TextLabel")
		title.Parent = tag
		title.Text = "Murderer"
		title.Size = UDim2.new(0,150,0,50)
		title.BackgroundColor3 = Color3.new(163,0,0)
		title.TextColor3 = Color3.new(255,255,255)
		title.TextScaled = true
		
	end
	
	if sheriff then
		
		print("Sheriff: " .. sheriff)
		local tag = Instance.new("BillboardGui")
		tag.Parent = game.Players[sheriff].Character.Head
		tag.Adornee = game.Players[sheriff].Character.Head
		tag.StudsOffset = Vector3.new(0,3,0)
		tag.Size = UDim2.new(0,150,0,50)
		tag.AlwaysOnTop = true
		
		local title = Instance.new("TextLabel")
		title.Parent = tag
		title.Text = "Sheriff"
		title.Size = UDim2.new(0,150,0,50)
		title.BackgroundColor3 = Color3.new(0,0,163)
		title.TextColor3 = Color3.new(255,255,255)
		title.TextScaled = true
		
	else
		print("Sheriff gun is dropped")
	end
end)


